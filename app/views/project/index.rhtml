   page.activeSection = "<%= @preferences.panel%>";
  
   referersTotalData=[<%= @referers_total -%>];
   referersUniqueData=[<%= @referers_unique -%>];
   referersRecentData=[<%= @referers_recent -%>];
   
  
//      referersTotalData=[];
//    referersUniqueData=[];
//    referersRecentData=[];
//   
  hitsWeekData=[<%= @hits_week %>];
  hitsWeekData=[300,500,200,300,500,600,450];
//   hitsWeekData=[1,2];
  hitsMonthData=[<%= @hits_month %>];
  
  
</script>

<!--[if lt IE 7.]>
<%= stylesheet_link_tag "main-ie" %>
<%= stylesheet_link_tag "project-ie" %>
<![endif]-->

<!-- styles for vertical centering-->
